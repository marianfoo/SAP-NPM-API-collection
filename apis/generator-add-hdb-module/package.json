    "yeoman.png",
    "features.json",
    "LICENSE",
    "NOTICE",
    "CHANGELOG.md",
    "npm-shrinkwrap.json",
    "README.md"
  ],
  "keywords": [
    "yeoman-generator",
    "hana-tooling"
  ],
  "dependencies": {
    "@sap-devx/inquirer-gui-folder-browser-plugin": "3.0.1",
    "@sap-devx/yeoman-ui-types": "1.14.1",
