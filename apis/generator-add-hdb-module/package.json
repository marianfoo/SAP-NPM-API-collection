    "@sap/mta-lib": "1.7.4",
    "@sap/xs-tools": "1.2.0",
    "command-exists": "1.2.9",
    "md5": "2.3.0",
    "properties-reader": "2.3.0",
    "rimraf": "5.0.5",
    "uuid": "9.0.1",
