  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://help.sap.com/viewer/c2b99f19e9264c4d9ae9221b22f6f589/",
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "generators/app/index.js",
