  "description": "ucl-provider",
  "main": "out/src/index.js",
  "license": "LICENSE.txt",
  "files": [
    "out",
    "modules.json"
  ],
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "lint": "eslint . --ext .ts --fix --max-warnings=0",
    "prepublishOnly": "npm run compile",
    "test": "nyc mocha -p tsconfig.json --opts ./mocha.opts",
    "format:fix": "prettier --write \"**/*.@(ts|js|json|md|yml)\"",
    "ci": "npm config set registry=https://int.repositories.cloud.sap/artifactory/api/npm/build-releases-npm && npm config list && npm install && npm run test && npm run lint"
  },
  "keywords": [
    "service-provider",
    "contributer"
  ],
  "dependencies": {
    "@sap/service-provider-apis": "1.12.2",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "0.1.3",
    "@typescript-eslint/eslint-plugin": "6.9.1",
    "@typescript-eslint/parser": "6.9.1",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "@types/chai": "4.3.5",
    "@types/chai-as-promised": "7.1.5",
    "@types/lodash": "4.14.191",
    "@types/mocha": "5.2.6",
    "@types/node": "13.13.52",
    "@types/sinon": "10.0.16",
    "chai": "4.3.10",
    "chai-as-promised": "7.1.1",
    "eslint": "8.53.0",
    "mocha": "6.2.3",
    "nyc": "15.1.0",
    "sinon": "17.0.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  }
}