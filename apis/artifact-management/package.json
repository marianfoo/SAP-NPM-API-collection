{
  "name": "@sap/artifact-management",
  "version": "1.29.0",
  "main": "dist/src/index.js",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "build": "npm run compile && npm run copy-templates && npm run copy-types",
    "test": "npm run lint && npm run test-unit",
    "test-unit": "bash ../../bin/run_unit_tests.sh",
    "clean": "rm -rf ./dist ./node_modules ./build ./out && rm -rf tsconfig.tsbuildinfo",
    "circular-deps": "./node_modules/madge/bin/cli.js --warning --circular --extensions ts src/.",
    "compile": "tsc -p tsconfig.json",
    "copy-templates": "gulp --gulpfile ./gulpfile.js copyTemplates",
    "copy-types": "gulp --gulpfile ./gulpfile.js copyTypes && bash ../../bin/update_types_version.sh",
    "lint": "eslint 'src/**/*.ts?(x)' --quiet",
    "lint-fix": "eslint 'src/**/*.ts?(x)' --quiet --fix",
    "premake-module": "bash ../../bin/clean_node_packages.sh",
    "make-module": "gulp --gulpfile ./gulpfile.js makeNodeModule",
    "postmake-module": "bash ../../bin/post_build_check.sh",
    "env": "env",
    "create-packages": "bash ../../bin/create_node_module.sh",
    "build:webpack": "webpack --config webpack.config.js"
  },
  "bin": {
    "dev-project": "dist/src/project-api/ProjectCLI.js"
  },
  "dependencies": {
    "@sap/artifact-management-base": "1.29.0",
    "@sap/artifact-management-fioriplugin": "1.29.0",
    "@sap/artifact-management-mdkplugin": "1.29.0",
    "@sap/artifact-management-hanaplugin": "1.29.0",
    "ajv": "8.12.0",
    "chokidar": "3.5.3",
    "js-yaml": "4.1.0",
    "jsdom": "^22.1.0",
    "lodash": "4.17.21",
    "micromatch": "4.0.5",
    "simple-git": "3.19.1",
    "uuid": "9.0.1",
    "vscode-languageserver-types": "3.17.3",
    "xml2js": "0.6.2",
    "yargs": "17.7.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "1.0.1",
    "@types/js-yaml": "3.12.5",
    "@types/lodash": "4.14.170",
    "@types/micromatch": "4.0.1",
    "@types/mocha": "8.0.4",
    "@types/node": "14.14.6",
    "@types/proxyquire": "1.3.28",
    "@types/sinon": "9.0.10",
    "@types/uuid": "8.3.4",
    "@types/vscode": "1.52.0",
    "@types/xml2js": "0.4.11",
    "@types/yargs": "15.0.10",
    "@typescript-eslint/eslint-plugin": "4.21.0",
    "@typescript-eslint/parser": "4.21.0",
    "@vscode-logging/types": "0.1.3",
    "copy-webpack-plugin": "9.0.1",
    "eslint": "7.23.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "gulp": "4.0.2",
    "gulp-json-editor": "2.5.6",
    "madge": "5.0.1",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "node-loader": "1.0.1",
    "nyc": "15.1.0",
    "prettier": "2.2.1",
    "proxyquire": "2.1.3",
    "shebang-loader": "0.0.1",
    "sinon": "9.2.2",
    "string-replace-loader": "3.0.3",
    "ts-loader": "9.2.4",
